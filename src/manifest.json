{
  "manifest_version": 2,

  "name": "Make Flowdock Great Again",
  "version": "0.3.1",
  "author": "Alex Hayes",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "description": "Takes Flowdock conversations back to a time when they didn't have so many icons.",

  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["jquery-3.2.1.slim.min.js", "script.js"],
    "matches": ["https://www.flowdock.com/*"]
  }],

  "homepage_url": "https://github.com/alexhayes/make-flowdock-great-again"

}
